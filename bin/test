#!/bin/bash
cargo clean
source <(cargo llvm-cov show-env --export-prefix)
cargo llvm-cov clean --workspace
cargo build --features gc-stress

target/debug/monoruby benchmark/app_fib.rb > monoruby.log
ruby benchmark/app_fib.rb > ruby.log
diff -s monoruby.log ruby.log
target/debug/monoruby benchmark/tarai.rb > monoruby.log
ruby benchmark/tarai.rb > ruby.log
diff -s monoruby.log ruby.log
target/debug/monoruby benchmark/quick_sort.rb 2> benchmark/quick_sort.disas > monoruby.log
ruby benchmark/quick_sort.rb > ruby.log
diff -s monoruby.log ruby.log
target/debug/monoruby benchmark/so_nbody.rb > monoruby.log
ruby benchmark/so_nbody.rb > ruby.log
diff -s monoruby.log ruby.log
target/debug/monoruby benchmark/bf.rb > monoruby.log
ruby benchmark/bf.rb > ruby.log
diff -s monoruby.log ruby.log

echo "checking plb2.."
target/debug/monoruby benchmark/plb2/nqueen.rb > monoruby.log
ruby --yjit benchmark/plb2/nqueen.rb > ruby.log
diff -s monoruby.log ruby.log
target/debug/monoruby benchmark/plb2/sudoku.rb > monoruby.log
ruby --yjit benchmark/plb2/sudoku.rb > ruby.log
diff -s monoruby.log ruby.log
target/debug/monoruby benchmark/plb2/matmul.rb > monoruby.log
ruby --yjit benchmark/plb2/matmul.rb > ruby.log
diff -s monoruby.log ruby.log
target/debug/monoruby benchmark/plb2/bedcov.rb > monoruby.log
ruby --yjit benchmark/plb2/bedcov.rb > ruby.log
diff -s monoruby.log ruby.log

target/debug/monoruby benchmark/app_aobench.rb > benchmark/monoruby.ppm && convert benchmark/monoruby.ppm benchmark/monoruby.jpg
ruby --yjit  benchmark/app_aobench.rb > benchmark/ruby.ppm && convert benchmark/ruby.ppm benchmark/ruby.jpg

target/debug/monoruby benchmark/so_mandelbrot.rb > benchmark/mandel1.ppm
target/debug/monoruby --no-jit benchmark/so_mandelbrot.rb > benchmark/mandel2.ppm
ruby --yjit benchmark/so_mandelbrot.rb > benchmark/mandel.ppm
diff -s benchmark/mandel.ppm benchmark/mandel1.ppm
diff -s benchmark/mandel.ppm benchmark/mandel2.ppm
rm benchmark/*.ppm

target/debug/monoruby ../optcarrot/bin/optcarrot -b --debug ../optcarrot/examples/Lan_Master.nes > monoruby.log
ruby --yjit ../optcarrot/bin/optcarrot -b --debug ../optcarrot/examples/Lan_Master.nes > ruby.log
diff -s monoruby.log ruby.log

target/debug/monoruby ../optcarrot/bin/optcarrot -b --opt --debug ../optcarrot/examples/Lan_Master.nes > monoruby.log
ruby --yjit ../optcarrot/bin/optcarrot -b --opt --debug ../optcarrot/examples/Lan_Master.nes > ruby.log
diff -s monoruby.log ruby.log
rm *.log

cargo llvm-cov report --lcov