#!/bin/bash

cargo build --release
cargo install --path monoruby
cd benchmark && benchmark-driver -o markdown quick_sort.yaml app_fib.rb tarai.rb so_mandelbrot.yml so_nbody.yml app_aobench.rb plb2/nqueen.rb plb2/sudoku.rb plb2/matmul.rb plb2/bedcov.rb --rbenv '3.4-dev; 3.4-dev --yjit; truffleruby+graalvm-24.0.1; truffleruby-24.0.1' -e ../target/release/monoruby -e "../target/release/monoruby --no-jit" --repeat-count 3
